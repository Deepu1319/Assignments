import java.util.*;
import java.util.ArrayList;

public class Q1List {

   public static void main(String[] args) {
       ArrayList<Float> list = new ArrayList<Float>();
       list.add(4.669F);
       list.add(8.5F);
       list.add(5.7F);

       Iterator<Float> itr = list.iterator();
       {
           float sum = 0;
           for (float i : list) {
               sum += i;
           }
           System.out.println(sum);

       }
   }
}


